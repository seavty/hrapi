@{
    Layout = "~/Views/Shared/Main/_Main.cshtml";
}

@using HRWeb.Utils
@model HRApi.Models.DTO.GetListDTO<HRApi.Models.DTO.Employee.EmployeePayrollDTO>


@foreach (var item in Model.results)
{
    <div class="card" style="margin-bottom:5px">
        <div class="card-body">
            <div> @item.fromDate - @item.toDate</div>
            <div>$ @item.prit_Salary.ThousandSeparator_Decimal() </div>
        </div>
    </div>

}




